                <script>
                  document.addEventListener('DOMContentLoaded', function () {
                    const form = document.querySelector('#bundle-form form');
                    const checkboxes = document.querySelectorAll('.bundle-addon');
                    const savingsCheckboxes = document.querySelectorAll('.savings-checkbox');
                    const variantSelectors = document.querySelectorAll('.bundle-variant-selector');
                    const optionalGifts = document.querySelectorAll('.gift-optional-label');
                    const totalItems = document.getElementById('bundle-total-items');
                    const totalPrice = document.getElementById('bundle-total-price');
                    const incompleteText = document.getElementById('bundle-incomplete-text');
                    const completeText = document.getElementById('upsell-header-complete');
                    const totalItemsText = document.querySelector('.bundle-total-items-text');
                    const totalSavings = document.getElementById('bundle-total-savings');
                    const compareAtPriceBlock = document.getElementById('bundle-compare-price');
                    const savingsBlock = document.getElementById('savings-block');
                    const basePrice = {% if is_euro == false %}{{ product.price | money_without_currency }}{% else %}{{ product.price | money_without_currency  | replace:',','x'  | replace:'.','' | replace:'x','.' }}{% endif %};

                    function updateBundle() {
                      let count = 1;
                      let price = basePrice;
                      let compareAtPrice = 0;
                      let allChecked = true;

                      checkboxes.forEach(box => {
                        if (box.checked) {
                          count++;
                          price += parseFloat(box.dataset.price);
                        } else {
                          allChecked = false;
                        }
                      });

                      if (allChecked) {
                        optionalGifts.forEach(g => {
                          oG = g.querySelector('.gift-optional');
                          oG.checked = true;
                          g.classList.remove('greyed-out');
                          incompleteText.style.display = 'none';
                          {% if block.settings.free_gifts_always.count == 0 %}
                            completeText.style.display = "flex";
                          {% endif %}
                        });
                      } else {
                        optionalGifts.forEach(g => {
                          oG = g.querySelector('.gift-optional');
                          oG.checked = false;
                          g.classList.add('greyed-out');
                          incompleteText.style.display = 'flex';
                          {% if block.settings.free_gifts_always.count == 0 %}
                            completeText.style.display = "none";
                          {% endif %}
                        });
                      }

                      savingsCheckboxes.forEach(box => {
                        if (box.checked && box.dataset.comparePrice) {
                          compareAtPrice += parseFloat(box.dataset.comparePrice);
                        }
                      });

                      totalItemsText.textContent = count > 1 ? "items:" : "item:";
                      totalItems.textContent = count;
                      totalPrice.textContent = totalPrice.textContent.replace(/[\d,]+(\.\d{2})?/, price.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ','));

                      if (compareAtPrice > price) {
                        totalSavings.textContent = totalSavings.textContent.replace(/[\d,]+(\.\d{2})?/, (compareAtPrice - price).toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ','));
                        compareAtPriceBlock.textContent = compareAtPriceBlock.textContent.replace(/[\d,]+(\.\d{2})?/, compareAtPrice.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ','));
                        savingsBlock.style.display = 'flex';
                      } else {
                        savingsBlock.style.display = 'none';
                      }
                    }

                    checkboxes.forEach(cb => cb.addEventListener('change', updateBundle));
                    savingsCheckboxes.forEach(cb => cb.addEventListener('change', updateBundle));
                    variantSelectors.forEach(sel => sel.addEventListener('change', updateBundle));
                    updateBundle();

                    if (form) {
                      const submitBtn = form.querySelector('[type="submit"]');
                      if (submitBtn) {
                        submitBtn.addEventListener('click', (e) => {
                          form.querySelectorAll('.bundle-cart-item').forEach((el) => el.remove());
                          form.querySelectorAll('input[name="product-id"]').forEach((el) => el.remove());

                          let anyChecked = false;
                          let index = 0;
                          let discount_index = 0;
                          let checks = [];
                          savingsCheckboxes.forEach((checkbox) => {
                            if (checkbox.checked) {
                              anyChecked = true;
                              const variantId = checkbox.getAttribute('value');
                              const checker = checkbox.getAttribute('checker');
                              const inputID = document.createElement('input');
                              inputID.type = 'hidden';
                              inputID.name = 'items[' + index + '][id]';
                              inputID.value = variantId;
                              inputID.classList.add('bundle-cart-item');
                              form.appendChild(inputID);
                              const inputQuantity = document.createElement('input');
                              inputQuantity.type = 'hidden';
                              inputQuantity.name = 'items[' + index + '][quantity]';
                              inputQuantity.value = 1;
                              inputQuantity.classList.add('bundle-cart-item');
                              form.appendChild(inputQuantity);
                              if(checker != null && checker.trim() != "") {
                                checks.push(checker);
                              }
                              index++;
                            }
                          });
                          if (!anyChecked) {
                            e.preventDefault();
                          } else {
                            fetch('/cart.js')
                            .then(res => res.json())
                            .then(cart => {
                              waitForCartChange(cart.items, () => {
                                checks.forEach(cks => appCks(cks));
                                const interval = setInterval(() => {
                                  fetch('/?sections=cart-drawer')
                                  .then(response => response.json())
                                  .then(data => {
                                    const html = new DOMParser().parseFromString(data['cart-drawer'], 'text/html');
                                    const node = html.querySelector('.drawer__inner');
                                    const container = document.querySelector('.drawer__inner');
                                    if (container) {
                                      container.innerHTML = node.innerHTML;
                                    }
                                  })
                                  .catch(error => {
                                    console.error('Failed to update cart drawer:', error);
                                  });
                                }, 300)
                                setTimeout(() => {
                                  clearInterval(interval)
                                }, 5000)
                              });
                            });
                          }
                        });
                      }
                    }
                  });
                </script>

                <style>
                  .greyed-out {
                    filter: grayscale(100%);
                    opacity: 0.6;
                  }
                  .bundle-block input[type="checkbox"] {
                    margin-right: 8px;
                  }
                  .bundle-variant-selector {
                    display: block;
                    margin-top: 4px;
